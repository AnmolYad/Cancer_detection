class Solution{
    public String removeConsecutiveCharacter(String S){
        
        String s = "";
        s += S.charAt(0);
        
        for (int i=1; i<S.length() ; i++)
        {
            if(S.charAt(i)!= S.charAt(i-1))
                s+= S.charAt(i) ;
            }
        return s;
    }
}
